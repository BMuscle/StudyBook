- if user_signed_in?
  %nav.navbar.navbar-expand.navbar-dark.bg-dark
    %ul.navbar-nav.ml-auto
      - if current_user.admin
        %li
          = link_to t('.admin'), admin_index_path, class: 'nav-link'
      %li
        = link_to t('.mypage'), user_path(current_user.nickname), class: 'nav-link'
      %li
        = link_to t('.log_out'), destroy_user_session_path, method: :delete, class: 'nav-link'
      %li
        = link_to t('.notes'), notes_path, class: 'nav-link'
      %li
        = link_to t('.my_lists'), my_lists_path, class: 'nav-link'
