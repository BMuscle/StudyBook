.row.m-0.p-0
  .left.col-2
    .left__content
      .left__header
        .left__label
          = t '.my_lists'
        = link_to t('.new'), top_index_path(select: 'new_my_listss'), class: "#{params[:select] == 'new_my_lists' ? 'left__link--active' : 'left__link--disable'}"
        = link_to t('.trend'), top_index_path(select: 'trend_my_lists'), class: "#{params[:select] == 'trend_my_lists' ? 'left__link--active' : 'left__link--disable'}"
        .left__label
          = t '.notes'
        = link_to t('.new'), top_index_path(select: 'new_notes'), class: "#{params[:select] == 'new_notes' ? 'left__link--active' : 'left__link--disable'}"
        = link_to t('.trend'), top_index_path(select: 'trend_notes'), class: "#{params[:select] == 'trend_notes' ? 'left__link--active' : 'left__link--disable'}"
  .main.col-8
    .top
      = render partial: @partial_path, locals: { notes: @notes, title: t(".#{@title}")}
  .right.col-2
