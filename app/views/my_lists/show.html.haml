.main
  .my-list
    .my-list__header#my_list_header
      = render partial: 'my_list_header', locals: { my_list: @my_list }
    .my-list__notes
      - @my_list.notes.each_with_index do |note, i|
        - unless i == 0
          %hr
        .my-list__note
          .my-list__note-main
            .my-list__note-title
              = note.title
          .my-list__note-sub
            .my-list__note-user
              = "#{t('general.by')} #{note.user.nickname}"
